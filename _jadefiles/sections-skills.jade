section.skills

  {% if site.data.settings.skills %}

  .skill-lockup

    {% for skill in site.data.settings.skills %}

    .skill-unit
      .skill-label{{ skill.name }}
      .skill-icons skill-{{ skill.value }}
        {% for i in (1..10) %}
          #[<span></span>]
        {% endfor %}

    {% endfor %}

  {% endif %}
